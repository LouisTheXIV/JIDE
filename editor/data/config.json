{  
    "highlight": {
        "enabled": "true",
        "string": "green",
        "number": "blue",
        "punctuation": "#FFFFFF",
        "brackets": "#FFFFFF",
        "error": "red"
    }, 

    "visuals": {
        "width": 850,
        "height": 750,
        "editor-bg": "black",
        "transparency" : 1
    },

    "text editor": {
        "auto-save": "false",
        "font-size": 16,
        "font": "Arial",
        "font-color": "white",
        "indentation": 4,
        "indent-new-lines": "true",
        "auto-bracket": "true",
        "auto-quote": "true"
    }
}